{
  "name": "vuerify",
  "private": false,
  "version": "1.0.0",
  "description": "Vuerify is a package that enables simple, customizable and secure form validation in Vue3",
  "repository": {
    "type": "git",
    "url": "https://github.com/karloci/vuerify.git"
  },
  "keywords": [
    "vuerify"
  ],
  "bugs": {
    "url": "https://github.com/karloci/vuerify/issues"
  },
  "homepage": "https://github.com/karloci/vuerify#readme",
  "author": "karloci",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "src/components/"
  ],
  "main": "./dist/vuerify.umd.cjs",
  "module": "./dist/vuerify.umd.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vuerify.js",
      "require": "./dist/vuerify.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.3.8"
  },
  "devDependencies": {
    "@types/node": "^20.10.2",
    "@vitejs/plugin-vue": "^4.5.0",
    "path": "^0.12.7",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.4",
    "vue-tsc": "^1.8.22"
  }
}
